{
  "name": "vite-vue-typescript-firebase-tailwind-msw-jest-starter",
  "version": "0.1.0",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "vite",
    "dev:css": "tailwindcss -i src/assets/css/tailwind.css -o src/tailwind.dist.css -w",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "test": "jest",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "0.22.0",
    "firebase": "9.1.1",
    "vue": "^3.0.5",
    "vue-router": "4.0.11",
    "vuex": "4.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.15.5",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/vue": "6.4.2",
    "@types/jest": "27.0.2",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "@vitejs/plugin-vue": "^1.3.0",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/vue3-jest": "27.0.0-alpha.1",
    "autoprefixer": "10.3.7",
    "babel-loader": "8.2.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-vue": "7.19.1",
    "husky": "7.0.2",
    "jest": "27.2.4",
    "lint-staged": "11.2.0",
    "msw": "0.35.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.3.9",
    "prettier": "2.4.1",
    "tailwindcss": "2.2.16",
    "ts-jest": "27.0.5",
    "typescript": "^4.3.2",
    "vite": "^2.4.4",
    "vue-loader": "16.8.1",
    "vue-tsc": "^0.2.2"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
